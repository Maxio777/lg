{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ],
    "no-string-literal": true,
    "no-inferrable-types": [
      false,
      "ignore-params"
    ],
    "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"]
  }
}
